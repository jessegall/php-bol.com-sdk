{
    "name": "jessegall/bol.com-sdk",
    "license": "MIT",
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "JesseGall\\BolComSDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JesseGall\\BolComSDK\\Tests\\": "tests/"
        },
        "files": [
            "tests/helpers.dev.php"
        ]
    },
    "authors": [
        {
            "name": "Jesse Gall",
            "email": "jesse-gall@hotmail.com"
        }
    ],
    "scripts": {
        "test": "@php -dxdebug.mode=coverage ./vendor/bin/phpunit --testdox --coverage-html=report --colors=auto",
        "test-filter": "@test --filter="
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^6.1",
        "spatie/ray": "^1.36"
    },
    "require": {
        "jessegall/resources": "dev-main",
        "jessegall/proxy": "dev-main",
        "guzzlehttp/guzzle": "^7.5",
        "vlucas/phpdotenv": "^5.4",
        "jessegall/invader": "^1.0"
    }
}
